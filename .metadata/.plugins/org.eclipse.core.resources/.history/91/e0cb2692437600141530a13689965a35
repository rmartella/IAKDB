:-consult("BaseConocimiento.pl").
:-consult("Planning.pl").
:-consult("Executing.pl").
:-loadBD.
%planning([agua=>mesa1,refresco=>mesa2],Plan).
planning([H|T],Plan):-
	estadoInicial([H|T],EI),
	write(EI),tab(2),nl,
	estadoObjetivo([H|T],EF),
	write(EF),tab(2),nl,
	encuentraAccionesAplicables(EF,EI,AEI),
	write(AEI),tab(2),nl,
	Nodo = nodo([estado=>EI,probabilidad=>0,recompenza=>0,costo=>0,heuristicaRecompenza=>1310,heuristicaProbabilidad=>818]),
	planeador(Nodo,EF,AEI,Plan),!.
	
	
	
	
	
	