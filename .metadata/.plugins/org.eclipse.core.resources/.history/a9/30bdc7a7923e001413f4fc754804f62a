%Clases
%lista_clases([object class object, clase1 class object, clase2 class clase1]).
:-op(15,xfx,class).
%busca_clase(Clase,[H|T]):-!.
busca_clase(Clase,[H|T],Padre):-
 class(Padre,Clase) = H,!;busca_clase(Clase,T,Padre).
push([],E,E):-!.
push([A|B],E,[A|D]):-
	push(B,E,D).		
consulta_extension(Clase,[],L):-!.
%consulta_extension(Clase,[H|T],L):-
%	busca_clase(Clase,[H|T],Padre),padre \= Clase ,write(Padre),nl,push([],Padre,L),consulta_extension(Padre,[H|T],L).
consulta_extension(Clase,[Clase|T],L):-!.
consulta_extension(Clase,[H|T],L):-
	busca_clase(Clase,[H|T],Padre),padre \= Clase, push([Padre],S,L).
