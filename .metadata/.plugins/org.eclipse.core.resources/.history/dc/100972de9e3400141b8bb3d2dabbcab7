%op(100,xfx,'=>').
%room(kitchen).
%is_in(apple, room(kitchen)).
%dynamic (is_in)/2.

%op(15,xfx,=>).
%valor(forma, [ color=>azul , forma=>esferica , peso=>ligero , material=>plastico] , X).
dynamic (=>)/2.
=>(X,Y):-
	write(X),tab(2),write(Y),nl.
	
valor(X,[], Y):-!.
valor(X,[X|T],Y):-
	Y = X,=>(X,Y),!.
valor(X,[H|T],Y):-
	valor(X,T,Y).	