
%dynamic (=>)/2.
%op(100,xfx,'=>').
%room(kitchen).
%is_in(apple, room(kitchen)).
%dynamic (is_in)/2.

%op(15,xfx,=>).
%valor(forma, [ color=>azul , forma=>esferica , peso=>ligero , material=>plastico] , X).
dynamic (=>)/2.
=>(X,Y):-
	=>(X,Y,Z),write(X),nl.
=>(X,Y,Z):-
	Z = Y, write(Z),nl.
	
valor(Atributo,[], X):-!.
valor(Atributo,[H|T],X):-
	X = =>(Atributo,Y),!.
valor(Atributo,[H|T],X):-
	valor(Atributo,T,X).	