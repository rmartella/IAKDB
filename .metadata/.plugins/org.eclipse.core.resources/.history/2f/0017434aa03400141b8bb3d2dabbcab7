
%dynamic (=>)/2.
%op(100,xfx,'=>').
%room(kitchen).
%is_in(apple, room(kitchen)).
%dynamic (is_in)/2.

%op(15,xfx,=>).
%valor(forma, [ color=>azul , forma=>esferica , peso=>ligero , material=>plastico] , X).
dynamic (=>)/2.
=>(X,Y):-
	=>(X,Y,Z).
=>(X,Y,Z):-
	Z = Y, write(Z).
	
valor(X,[], Y):-!.
valor(Atributo,[X|T],Y):-
	Y = Atributo,!.
valor(Atributo,[H|T],X):-
	valor(Atributo,T,X).	